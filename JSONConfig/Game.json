{
	"table":"chess_game",
	"constructBy":"id",
	"columns":{
		"id":"int auto_increment not null primary key",
		"fen_id":{
			"db":"int"
		},
		"fen":{
			"db": "varchar(128)",
			"access": "rw"
		},
		"database_id":{
			"db":"int",
			"access":"rw",
            "references": "chess_database(id) on delete cascade"
		},
		"created_by":{
			"db":"int",
			"access":"rw"
		},
		"creator":{
			"class":"Player",
			"fk":"created_by",
			"get":"getFullName"
		},
		"result":{
			"db":"varchar(64) default '*'",
			"access":"rw"
		},
		"metadata":{
			"class":"MetadataCollection",
			"set":"setMetadata"
		},
		"moves":{
			"class":"moves",
			"set":"setMoves"
		}
	},
	"indexes":["database_id", "created_by"]
}